<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="study" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>kz.study.jdbc.Jdbc/arma</jta-data-source>
        <class>kz.study.entity.TestType</class>
        <class>kz.study.entity.AlphLinks</class>
        <class>kz.study.entity.GameResult</class>
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
            <property name="eclipselink.kz.study.jdbc.Jdbc.url" value="kz.study.jdbc.Jdbc:mysql://10.1.1.26:3306/arma"/>
            <property name="eclipselink.kz.study.jdbc.Jdbc.driver" value="com.mysql.jdbc.Driver"/>
        </properties>
    </persistence-unit>
</persistence>
